{
	"presets": ["next/babel"],
	"plugins": [
		[
			"module-resolver",
			{
				"root": ["./src"],
				"alias": {
					"@assets": "./src/assets",
					"@components": "./src/components",
					"@constants": "./src/constants",
					"@containers": "./src/containers",
					"@context": "./src/context",
					"@hooks": "./src/hooks",
					"@styles": "./src/styles",
					"@type": "./src/type",
					"@utils": "./src/utils"
				}
			}
		]
	]
}
